{
  "name": "@percy/selenium-webdriver",
  "description": "Selenium client library for visual testing with Percy",
  "version": "2.0.5",
  "license": "MIT",
  "author": "Perceptual Inc.",
  "repository": "https://github.com/percy/percy-selenium-js",
  "keywords": [
    "selenium",
    "percy",
    "visual testing"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "main": "index.js",
  "types": "types/index.d.ts",
  "files": [
    "*.js",
    "types/index.d.ts"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "lint": "eslint --ignore-path .gitignore .",
    "test": "cross-env MOZ_HEADLESS=1 NODE_ENV=test percy exec --testing -- jasmine --config=./test/jasmine.json",
    "test:coverage": "nyc yarn test",
    "test:types": "tsd"
  },
  "dependencies": {
    "@percy/sdk-utils": "^1.28.0",
    "node-request-interceptor": "^0.6.3"
  },
  "devDependencies": {
    "@percy/cli": "^1.28.0",
    "@types/selenium-webdriver": "^4.0.9",
    "cross-env": "^7.0.2",
    "eslint": "^8.27.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "geckodriver": "^3.0.2",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "nyc": "^15.1.0",
    "selenium-webdriver": "~4.10.0",
    "tsd": "^0.24.1"
  }
}
